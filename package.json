{
  "name": "react-base",
  "version": "0.0.1",
  "description": "Modern React base",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server",
    "dev": "npm run clean && nodemon -w webpack.config.js -w watch-dev.js -w server.js watch-dev",
    "build-dev": "webpack --verbose --colors --display-error-details --config",
    "build-prod": "NODE_ENV=production npm run build-dev",
    "build": "npm run clean && npm run build-prod",
    "lint": "eslint src",
    "clean": "rimraf src/assets/build src/shared/build"
  },
  "author": "",
  "dependencies": {
    "babel": "^5.8.20",
    "babel-runtime": "^5.8.20",
    "koa": "^1.0.0",
    "koa-static": "^1.4.9",
    "react": "^0.13.3",
    "react-document-meta": "^0.1.4",
    "react-redux": "^0.9.0",
    "react-router": "^1.0.0-beta3",
    "redux": "^1.0.0-rc",
    "redux-actions": "^0.7.0",
    "redux-thunk": "^0.1.0",
    "serialize-javascript": "^1.0.0",
    "source-map-support": "^0.3.2"
  },
  "devDependencies": {
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.16.0",
    "cssnext": "^1.8.3",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.1.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.2",
    "lodash.merge": "^3.3.2",
    "nodemon": "^1.4.0",
    "postcss-loader": "^0.5.1",
    "react-hot-loader": "^2.0.0-alpha-1",
    "redux-devtools": "^1.0.2",
    "rimraf": "^2.4.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "webpack-manifest-plugin": "^0.4.0"
  }
}
